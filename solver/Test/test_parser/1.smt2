(set-logic QF_NRA)
(declare-fun x_0 () Real)
(declare-fun x_1 () Real)
(declare-fun x_2 () Real)
(assert (< (- x_0 x_1 x_2 (- x_1 x_2) ) 1))
(assert (< (+ x_0 x_1 x_2 (- x_1 x_2) ) 1))
(assert (< (* x_0 x_1 (- x_1 x_2) ) 1))
(assert (< (/ x_0 x_1 (- x_1 x_2) ) 1))
(assert (and (< x_0 1) (< x_1 1) (< x_2 1)))
(assert (or (< x_0 1) (< x_1 1) (< x_2 1)))
(check-sat)
(exit)
